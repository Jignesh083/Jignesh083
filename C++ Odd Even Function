#include <iostream>
using namespace std;

bool isEven_Odd(int a)
{
    if (a & 1)
    {
        return 0;
    }

    return 1;
}

int main()
{
    int num;
    cin >> num;

    if (isEven_Odd(num))
    {
        cout << "Number is Even" << endl;
    }
    else
    {
        cout << "Number is Odd";
    }

    return 0;
}
