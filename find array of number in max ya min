#include <stdio.h>
#include <conio.h>

int main()
{
	int arr[100];
	int size,max,min;
	
	printf("Enter the number of arrays:");
	scanf("%d",&size);
	
	printf("Enter the elements of %d\n",size);
	for(int i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	
	max = arr[0];
	min = arr[0];
	
	for(int i=0;i<size;i++)
	{
		if(arr[i]>max)
		{
			max = arr[i];
		}
		if(arr[i]<min)
		{
			min = arr[i];
		}
		
	}
	
	printf("The elements is max = %d\n",max);
	printf("The elements is min = %d",min);
}
