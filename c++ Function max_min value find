#include <iostream>
#include <climits>
using namespace std;

int getMin(int num[], int n)
{
    int minx = INT_MAX;

    for (int i = 0; i < n; i++)
    {
        minx = min(minx, num[i]);
        //     if (num[i] < min)
        //     {
        //         min = num[i];
        //     }
    }
    return minx;
}

int getMax(int num[], int n)
{
    int maxi = INT_MIN;

    for (int i = 0; i < n; i++)
    {
        maxi = max(maxi, num[i]);
        // if (num[i] > max)
        // {
        //     max = num[i];
        // }
    }
    return maxi;
}

int main()
{
    int size;
    cin >> size;

    int num[100];

    for (int i = 0; i < size; i++)
    {
        cin >> num[i];
    }
    cout << "Maximum Value is " << getMax(num, size) << endl;
    cout << "Minimum value is " << getMin(num, size) << endl;
}
