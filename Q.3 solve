#include <iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter the value";
    cin >> n;

    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j < n; j++)
        {

            if (i == 1 || i == 3)
            {
                cout << "12";
            }
            else
            {
                cout << "21";
            }
        }
        cout << endl;
    }
}
