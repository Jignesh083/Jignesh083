#include <iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter the value:";
    cin >> n;
    int result = 0;
    int power = 1;
    while (n > 0)
    {
        int paritydigit = n % 2;
        result += paritydigit * power;
        power *= 10;
        n /= 2;
    }

    cout << result << endl;

    return 0;
}
